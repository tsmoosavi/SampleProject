<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sampleproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/doctoricon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/doctoricon"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleProject">
        <activity
            android:name=".firstPageOfHospital"
            android:exported="false" />
        <activity
            android:name=".DoctorActivity"
            android:exported="true"
            android:parentActivityName=".firstPageOfHospital">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>